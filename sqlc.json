{
  "version": "2",
  "sql": [
    {
      "engine": "postgresql",
      "queries": ["db/query.sql"],
      "schema": ["db/schema.sql"],
      "gen": {
        "go": {
          "out": "dao",
          "package": "dao",
          "sql_package": "database/sql",
          "overrides": [
            {
              "db_type": "pg_catalog.numeric",
              "go_type": {
                "import": "github.com/shopspring/decimal",
                "type": "Decimal"
              }
            },
            {
              "db_type": "numeric",
              "go_type": {
                "import": "github.com/shopspring/decimal",
                "type": "Decimal"
              }
            },
            {
              "db_type": "decimal",
              "go_type": {
                "import": "github.com/shopspring/decimal",
                "type": "Decimal"
              }
            }
          ]
        }
      }
    }
  ]
}